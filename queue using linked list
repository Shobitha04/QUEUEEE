class Node{
int data;
Node next;
Node(int x){
data = x;
next = null;}
}
class Queue{
Node front, rear;
Queue(){ front=rear=null;}
void enque(int x)
{
Node temp = new Node(x);
if(front==null){
front=rear=temp;
return;
}
rear.next=temp;
rear=temp; //inserting new node at the rear end of the queue
}
void deque()
{
Node front,rear;
if(front==null)
  return;
  front=front.next;
if(front==null){
    rear=null;
}
}
}



